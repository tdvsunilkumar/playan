ALTER TABLE `cto_tax_credit_management` ADD `tfoc_is_applicable` INT(11) NOT NULL DEFAULT '0' COMMENT '1=Business Permit, 2=Real Property, 3=Engineering, 4=Occupancy,5=Planning & Devt., 6=Health & Safety, 7=Community Tax, 8=Burial Permit, 9=Miscellaneous' AFTER `pcs_id`, ADD `rpt_category` INT(11) NOT NULL DEFAULT '0' COMMENT '1=Building, 2=Land, 3=Machinery' AFTER `tfoc_is_applicable`;