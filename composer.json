{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "composer-runtime-api": "^2.0",
        "anandsiddharth/laravel-paytm-wallet": "^2.0",
        "anhskohbo/no-captcha": "^3.3",
        "barryvdh/laravel-dompdf": "*",
        "coingate/coingate-php": "^3.0",
        "doctrine/dbal": "^2.10",
        "elibyy/tcpdf-laravel": "^10.0",
        "fideloper/proxy": "^4.2",
        "firebase/php-jwt": "^5.5",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "jenssegers/agent": "^2.6",
        "kkomelin/laravel-translatable-string-exporter": "^1.10",
        "kwn/number-to-words": "^2.7",
        "laravel/framework": "^8.54",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^2.13",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.1",
        "league/flysystem-sftp": "^1.1",
        "livewire/livewire": "^2.5",
        "maatwebsite/excel": "^3.1",
        "mashape/unirest-php": "^3.0",
        "mercadopago/dx-php": "^2.4",
        "milon/barcode": "^8.0",
        "mollie/mollie-api-php": "^2.37",
        "mpdf/mpdf": "^8.2",
        "munafio/chatify": "^1.2",
        "nesbot/carbon": "^2.66",
        "obydul/laraskrill": "^1.1",
        "paymentwall/paymentwall-php": "^2.2",
        "paypal/rest-api-sdk-php": "^1.14",
        "php-http/curl-client": "^2.2",
        "phpoffice/phpexcel": "dev-master",
        "pusher/pusher-php-server": "7.0",
        "rachidlaasri/laravel-installer": "^4.1",
        "rats/zkteco": "^002.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "snapappointments/bootstrap-select": "^1.13",
        "spatie/laravel-permission": "^3.13",
        "stripe/stripe-php": "^7.36",
        "symfony/process": "^5.4",
        "tecnickcom/tcpdf": "^6.6",
        "twilio/sdk": "^6.32"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.3",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php",
            "app/Helpers/Helper-global.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
